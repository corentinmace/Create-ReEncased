{
  "schemaVersion": 1,
  "id": "createencasedfabric",
  "version": "${version}",
  "name": "Create: ReEncased",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "kuha_sr"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/create-fabric-addon-template"
  },
  "license": "CC0",
  "icon": "assets/createencasedfabric/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kuhasr.createencasedfabric.CreateReEncasedMod"
    ],
    "client": [
      "com.kuhasr.createencasedfabric.CreateReEncasedModClient"
    ]
  },
  "mixins": [
